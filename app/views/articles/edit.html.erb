<h1>Editing article</h1>

<%= form_for(@edit_article) do |f| %>
  <div>
    <%= f.label :Title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :Content %>
    <%= f.text_area :content %>
  </div>
  <%= f.submit %>
<% end %>

<%= link_to "Show", articles_path %> || <%= link_to "Back", article_path %>
